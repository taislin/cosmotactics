{
    "applicationId": "js.neutralino.cosmotactics",
    "defaultMode": "window",
    "url": "/app/",
    "port": 0,
    "enableServer": true,
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": [
        "app.*"
    ],
    "modes": {
        "window": {
            "title": "CosmoTactics",
            "width": 1230,
            "height": 860,
            "minWidth": 1024,
            "minHeight": 768,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "./app/icons/icon.png",
            "enableInspector": false,
            "borderless": false,
            "maximize": false
        }
    },
    "cli": {
        "binaryName": "cosmotactics",
        "resourcesPath": "./app/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "./app/assets/neutralino.js",
        "binaryVersion": "6.2.0",
        "clientVersion": "6.2.0"
    }
}